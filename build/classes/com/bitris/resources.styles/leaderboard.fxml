<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?scenebuilder-stylesheet styleSheet.css?>

<Pane fx:id="vista3" xmlns:fx="http://javafx.com/fxml" fx:controller="com.bitris.controllers.LeaderboardController">
    <children>
        <Label id="writing" text="Leaderboard"
                    layoutX="540" layoutY="2" />
        <Button id="big-btn" mnemonicParsing="false" onAction="#menuPane" text="Back"
                    layoutX="15" layoutY="300" />
        <BorderPane styleClass="scoreBoard" fx:id="scoreBoard" layoutX="400" layoutY="40">
            <padding><Insets top="50" right="50" bottom="50" left="50"/></padding>
                <center>
                    <GridPane id="leaderBoard" fx:id="leaderBoard" hgap="30" vgap="30" />
                </center>
        </BorderPane>
    </children>
</Pane>
